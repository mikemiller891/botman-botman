{
    "name": "botman/botman",
    "license": "MIT",
    "description": "Create messaging bots in PHP with ease.",
    "keywords": [
        "Bot",
        "BotMan",
        "Chatbot"
    ],
    "homepage": "http://github.com/botman/botman",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "spatie/macroable": "^1.0",
        "mpociot/pipeline": "^1.0",
        "opis/closure": "^3.5"
    },
    "require-dev": {
        "ext-curl": "*",
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.5",
        "doctrine/cache": "^1.10",
        "symfony/cache": "^5.1"
    },
    "suggest": {
        "doctrine/cache": "Use any Doctrine cache driver for cache",
        "symfony/cache": "Use any Symfony cache driver for cache"
    },
    "autoload": {
        "psr-4": {
            "BotMan\\BotMan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BotMan\\BotMan\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        },
        "laravel": {
            "providers": [
                "BotMan\\BotMan\\BotManServiceProvider"
            ],
            "aliases": {
                "BotMan": "BotMan\\BotMan\\Facades\\BotMan"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs": "php-cs-fixer fix"
    }
}
